/* components/searchbar/searchbar.wxss */
.searchbar {
  background: #41be57;
  padding: 20rpx;
}
.search-navigator {
  width: 100%;
  height: 60rpx;
  background-color:#fff;
  border-radius: 10rpx;
  background-image: url("https://s2.ax1x.com/2019/04/15/Aj576g.png");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;

}
.search-input-group {
  width: 100%;
  height: 60rpx;
  background-color:#fff;
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.search-input {
  min-height: 40rpx;
  height: 40rpx;
  font-size: 12px;
}